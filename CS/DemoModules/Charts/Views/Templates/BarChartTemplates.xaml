<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns:templateView="clr-namespace:DemoCenter.Maui.Charts.Views"
                    x:Class="DemoCenter.Maui.Charts.Templates.BarChartTemplates">
    
    <DataTemplate x:Key="sideBySideRangeBarChartTemplate">
        <templateView:SideBySideRangeBarChartContainer/>
    </DataTemplate>
    <DataTemplate x:Key="rangeBarChartTemplate">
        <templateView:RangeBarChartContainer/>
    </DataTemplate>
    <DataTemplate x:Key="barChartTemplate">
        <templateView:BarChartContainer/>
    </DataTemplate>
    <DataTemplate x:Key="populationPyramidTemplate">
        <templateView:PopulationPyramidContainer/>
    </DataTemplate>
    <DataTemplate x:Key="stackedBarChartTemplate">
        <templateView:StackedBarChartContainer/>
    </DataTemplate>
    <DataTemplate x:Key="sideBySideStackedBarChartTemplate">
        <templateView:SideBySideStackedBarChartContainer/>
    </DataTemplate>
    <DataTemplate x:Key="fullStackedBarChartTemplate">
        <templateView:FullStackedBarChartContainer/>
    </DataTemplate>
    <DataTemplate x:Key="sideBySideFullStackedBarChartTemplate">
        <templateView:SideBySideFullStackedBarChartContainer/>
    </DataTemplate>
    <DataTemplate x:Key="rotatedStackedBarChartTemplate">
        <templateView:RotatedStackedBarChartContainer/>
    </DataTemplate>
    <DataTemplate x:Key="rotatedSideBySideStackedBarChartTemplate">
        <templateView:RotatedSideBySideStackedBarChartContainer/>
    </DataTemplate>

</ResourceDictionary>
