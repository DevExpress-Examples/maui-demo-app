<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns:templateView="clr-namespace:DemoCenter.Maui.Charts.Views"
                    x:Class="DemoCenter.Maui.Charts.Templates.LineChartTemplates">

    <DataTemplate x:Key="lineChartTemplate">
        <templateView:LineChartContainer/>
    </DataTemplate>
    <DataTemplate x:Key="scatterChartTemplate">
        <templateView:ScatterChartContainer/>
    </DataTemplate>
    <DataTemplate x:Key="stepLineChartTemplate">
        <templateView:StepLineChartContainer/>
    </DataTemplate>
    <DataTemplate x:Key="splineChartTemplate">
        <templateView:SplineChartContainer/>
    </DataTemplate>

</ResourceDictionary>
